// =====================================================
// 测试错误类型 11: 操作数类型不匹配
// =====================================================

// =============== 正确组 (不触发错误) ===============
int main() {
    // 正确: 同类型运算
    int a = 10;
    int b = 20;
    int c = a + b;
    int d = a * b;
    
    float f1 = 1.5;
    float f2 = 2.5;
    float f3 = f1 + f2;
    
    return 0;
}

// =============== 对照组 (触发 Error 11) ===============
int error_case() {
    float f = 1.5;
    int i = 10;
    
    // Error 11: 操作数类型不匹配 (int 和 float 不能混合运算)
    int mixed = f + i;  // Error 11: Type mismatch
    
    float f2 = 2.5;
    int result = i * f2;  // Error 11: Type mismatch
    
    int x = f + f2 + i;  // Error 11: Type mismatch
    
    return 0;
}