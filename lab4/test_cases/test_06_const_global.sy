// 常量与全局变量测试：const, 全局变量, 作用域
// 覆盖规则：ConstDecl, ConstDef, ConstInitVal, ConstExp
const int MAX_N = 100;
int global_var = 10;
const int CONST_ARR[2] = {10, 20};

int main() {
    int student_id = 20220001;
    const int local_const = 5;
    int i = 0;
    int sum = 0;
    
    while (i < local_const) {
        sum = sum + global_var;
        i = i + 1;
    }
    
    return sum + CONST_ARR[1]; // 50 + 20 = 70
}