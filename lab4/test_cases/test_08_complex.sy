// 综合测试：快速排序算法实现
// 覆盖规则：综合运用
int partition(int arr[], int low, int high) {
    int pivot = arr[high];
    int i = low - 1;
    int j = low;
    while (j < high) {
        if (arr[j] < pivot) {
            i = i + 1;
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
        j = j + 1;
    }
    int temp = arr[i + 1];
    arr[i + 1] = arr[high];
    arr[high] = temp;
    return i + 1;
}

void quick_sort(int arr[], int low, int high) {
    if (low < high) {
        int pi = partition(arr, low, high);
        quick_sort(arr, low, pi - 1);
        quick_sort(arr, pi + 1, high);
    }
}

int main() {
    int student_id = 20220001;
    int arr[10] = {10, 7, 8, 9, 1, 5, 2, 4, 3, 6};
    quick_sort(arr, 0, 9);
    
    // Check if sorted
    int i = 0;
    while (i < 9) {
        if (arr[i] > arr[i+1]) return -1; // Fail
        i = i + 1;
    }
    return 0; // Success
}